:javascript
  // Initialize color controls
  const backgroundColorPicker =
    document.getElementById("background-color");
  const backgroundHexInput =
    document.getElementById("background-hex");
  const foregroundColorPicker =
    document.getElementById("foreground-color");
  const foregroundHexInput =
    document.getElementById("foreground-hex");

  // Current active SVG
  let currentSvg = 'original';

  // URL parameter management
  function getQueryParams() {
    const urlParams = new URLSearchParams(window.location.search);
    return {
      bg: urlParams.get('bg'),
      fg: urlParams.get('fg'),
      logo: urlParams.get('logo')
    };
  }

  function setQueryParams(bgColor, fgColor, logoVersion) {
    const url = new URL(window.location);
    url.searchParams.set('bg', bgColor.replace('#', ''));
    url.searchParams.set('fg', fgColor.replace('#', ''));
    url.searchParams.set('logo', logoVersion);
    window.history.replaceState({}, '', url);
  }

  function initializeColorsFromParams() {
    const params = getQueryParams();

    function setColorFromParam(param, colorPicker, hexInput, defaultColor) {
      const color = param && /^[0-9A-Fa-f]{6}$/.test(param) ? '#' + param : defaultColor;
      colorPicker.value = color;
      hexInput.value = color;
    }

    setColorFromParam(params.bg, backgroundColorPicker, backgroundHexInput, "#222222");
    setColorFromParam(params.fg, foregroundColorPicker, foregroundHexInput, "#eeeeee");

    // Initialize logo version from URL
    if (params.logo === 'contract') {
      document.getElementById('logo-contract').checked = true;
      currentSvg = 'contract';
    } else {
      document.getElementById('logo-original').checked = true;
      currentSvg = 'original';
    }
    switchLogo();
  }

  // Store references to elements by type
  let backgroundElements = [];
  let foregroundElements = [];

  // Store original element categorization to avoid re-scanning after color changes
  let originalElementCategories = new Map();

  // Initialize element categorization based on original colors
  function initializeElements() {
    // Clear existing arrays
    backgroundElements = [];
    foregroundElements = [];

    // Get the current active SVG
    const activeSvg = document.querySelector('svg.active');
    if (!activeSvg) return;

    const svgId = activeSvg.id;

    // If we haven't categorized this SVG before, scan it
    if (!originalElementCategories.has(svgId)) {
      const categories = { background: [], foreground: [] };

      // Add the background rectangle(s)
      const svgBackground = activeSvg.querySelector("#svg-background, #svg-background-contract");
      if (svgBackground) {
        categories.background.push(svgBackground);
      }

      // Find all colored SVG elements in the active SVG only
      activeSvg.querySelectorAll("*").forEach((element) => {
        if (element.id === "svg-background" || element.id === "svg-background-contract") return;

        const style = element.getAttribute("style");
        const fill = element.getAttribute("fill");

        // Check for background color elements (#222222)
        if ((style && style.match(/fill\s*:\s*#222222/i)) ||
          (fill && fill.toLowerCase() === "#222222")) {
          categories.background.push(element);
        }
        // Check for foreground color elements (#eeeeee)
        else if ((style && style.match(/fill\s*:\s*#eeeeee/i)) ||
          (fill && fill.toLowerCase() === "#eeeeee")) {
          categories.foreground.push(element);
        }
      });

      originalElementCategories.set(svgId, categories);
    }

    // Use the stored categorization
    const categories = originalElementCategories.get(svgId);
    backgroundElements = [...categories.background];
    foregroundElements = [...categories.foreground];
  }

  // Update colors function
  function updateColors() {
    const backgroundColor = backgroundColorPicker.value;
    const foregroundColor = foregroundColorPicker.value;

    function updateElementColor(element, color) {
      if (element.id === "svg-background") {
        element.setAttribute("fill", color);
        return;
      }

      const style = element.getAttribute("style");
      const fill = element.getAttribute("fill");

      if (style && style.includes("fill:")) {
        const newStyle = style.replace(/fill\s*:\s*[^;]+/gi, `fill: ${color}`);
        element.setAttribute("style", newStyle);
      } else if (fill) {
        element.setAttribute("fill", color);
      } else {
        element.setAttribute("style", `fill: ${color}`);
      }
    }

    backgroundElements.forEach(el => updateElementColor(el, backgroundColor));
    foregroundElements.forEach(el => updateElementColor(el, foregroundColor));

    // Update hex inputs to stay synced with color pickers
    backgroundHexInput.value = backgroundColor;
    foregroundHexInput.value = foregroundColor;

    // Update URL parameters
    setQueryParams(backgroundColor, foregroundColor, currentSvg);
  }

  // Validate and apply hex input
  function applyHexColor(hexInput, colorPicker) {
    const hexValue = hexInput.value.trim();
    const hexRegex = /^#[0-9A-Fa-f]{6}$/;

    if (hexRegex.test(hexValue)) {
      colorPicker.value = hexValue;
      updateColors();
    } else {
      // Reset to current color picker value if invalid
      hexInput.value = colorPicker.value;
    }
  }

  // Reset to default colors
  function resetColors() {
    backgroundColorPicker.value = "#222222";
    foregroundColorPicker.value = "#eeeeee";
    updateColors();
  }

  // Event listeners for color pickers
  backgroundColorPicker.addEventListener("input", updateColors);
  foregroundColorPicker.addEventListener("input", updateColors);

  // Event listeners for hex inputs
  function addHexListeners(hexInput, colorPicker) {
    hexInput.addEventListener("blur", () => applyHexColor(hexInput, colorPicker));
    hexInput.addEventListener("keypress", (e) => {
      if (e.key === "Enter") applyHexColor(hexInput, colorPicker);
    });
  }

  addHexListeners(backgroundHexInput, backgroundColorPicker);
  addHexListeners(foregroundHexInput, foregroundColorPicker);

  // Switch logo function
  function switchLogo() {
    const originalRadio = document.getElementById('logo-original');
    const contractRadio = document.getElementById('logo-contract');
    const originalSvg = document.getElementById('svg-original');
    const contractSvg = document.getElementById('svg-contract');

    if (originalRadio.checked) {
      originalSvg.classList.add('active');
      contractSvg.classList.remove('active');
      currentSvg = 'original';
    } else if (contractRadio.checked) {
      contractSvg.classList.add('active');
      originalSvg.classList.remove('active');
      currentSvg = 'contract';
    }

    // Re-initialize elements for the new SVG and update colors
    initializeElements();
    updateColors();

    // Update URL parameters with new logo version
    setQueryParams(backgroundColorPicker.value, foregroundColorPicker.value, currentSvg);
  }

  // Event listeners for logo version radio buttons
  document.getElementById('logo-original').addEventListener('change', switchLogo);
  document.getElementById('logo-contract').addEventListener('change', switchLogo);

  // Download PNG function
  function downloadPNG() {
    const svg = document.querySelector("svg.active");
    const svgData = new XMLSerializer().serializeToString(svg);

    // Get user-specified resolution
    const resolutionInput =
      document.getElementById("resolution-input");
    const resolution = parseInt(resolutionInput.value) || 1354;

    // Create a canvas to render the SVG
    const canvas = document.createElement("canvas");
    const ctx = canvas.getContext("2d");
    const img = new Image();

    // Set canvas size to user-specified resolution
    canvas.width = resolution;
    canvas.height = resolution;

    img.onload = function () {
      // Draw the SVG image to canvas at user-specified resolution
      ctx.drawImage(img, 0, 0, resolution, resolution);

      // Convert canvas to PNG and download
      canvas.toBlob(function (blob) {
        const url = URL.createObjectURL(blob);
        const a = document.createElement("a");
        a.href = url;
        a.download = `ksru-logo-${currentSvg}-custom-colors.png`;
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
        URL.revokeObjectURL(url);
      }, "image/png");
    };

    // Convert SVG to data URL and load into image
    const svgBlob = new Blob([svgData], {
      type: "image/svg+xml;charset=utf-8",
    });
    const url = URL.createObjectURL(svgBlob);
    img.src = url;
  }

  // Download SVG function
  function downloadSVG() {
    const svg = document.querySelector("svg.active");
    const svgData = new XMLSerializer().serializeToString(svg);

    // Create a blob with the SVG data
    const svgBlob = new Blob([svgData], {
      type: "image/svg+xml;charset=utf-8",
    });

    // Create download link
    const url = URL.createObjectURL(svgBlob);
    const a = document.createElement("a");
    a.href = url;
    a.download = `ksru-logo-${currentSvg}-custom-colors.svg`;
    document.body.appendChild(a);
    a.click();
    document.body.removeChild(a);
    URL.revokeObjectURL(url);
  }

  // Initialize on page load
  document.addEventListener("DOMContentLoaded", () => {
    initializeElements();
    initializeColorsFromParams();
    updateColors();
  });
